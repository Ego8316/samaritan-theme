{
	"name": "Samaritan",
	"colors": {
		"editor.background": "#f0f0f0",
		"editor.foreground": "#000000",
		"list.activeSelectionIconForeground": "#d32f2f",
		"editorCursor.foreground": "#ff0000",
		"editor.selectionBackground": "#ffffff",
		"editor.lineHighlightBackground": "#d1d1d1",
		"editorLineNumber.foreground": "#e00000",
		"editorLineNumber.activeForeground": "#ff0000",
		"terminal.foreground": "#000000",
		"terminal.background": "#ffffff",
		"statusBar.background": "#b71c1c",
		"statusBar.foreground": "#ffffff",
		"statusBar.noFolderBackground": "#d32f2f",
		"statusBar.debuggingBackground": "#ff3d3d",
		"statusBar.border": "#880000",
		"activityBarBadge.background": "#ff3d3d",
		"activityBarBadge.foreground": "#ffffff",
		"sideBar.background": "#d9dada",
		"sideBar.foreground": "#000000",
		"sideBar.border": "#cccccc",
		"list.inactiveSelectionBackground": "#cbcccc",
		"list.inactiveSelectionForeground": "#000000",
		"list.activeSelectionBackground": "#d32f2d",
		"list.focusOutline": "#cccccc",
		"gitDecoration.modifiedResourceForeground": "#000000",
		"gitDecoration.addedResourceForeground": "#000000",
		"gitDecoration.deletedResourceForeground": "#000000",
		"gitDecoration.untrackedResourceForeground": "#000000",
		"gitDecoration.ignoredResourceForeground": "#494949"
	},
	"tokenColors": [
	{
		"name": "Comments",
		"scope": [
			"comment",
			"punctuation.definition.comment"
		],
		"settings": {
		"fontStyle": "italic",
			"foreground": "#2b2b2b"
		}
	},
	{
		"name": "Keywords",
		"scope": [
			"keyword",
			"storage"
		],
		"settings": {
			"foreground": "#ff0000"
		}
	},
	{
		"name": "Variables",
		"scope": [
			"variable",
			"support.variable"
		],
		"settings": {
			"foreground": "#000000"
		}
	},
	{
		"name": "Functions",
		"scope": [
			"entity.name.function",
			"support.function"
		],
		"settings": {
			"foreground": "#ff0000"
		}
	},
	{
		"name": "Strings",
		"scope": "string",
		"settings": {
			"foreground": "#dd1f1f"
		}
	},
	{
		"name": "Numbers",
		"scope": [
			"constant.numeric",
			"constant.character"
		],
		"settings": {
			"foreground": "#000000"
		}
	},
	{
		"name": "Punctuation",
		"scope": "punctuation",
		"settings": {
			"foreground": "#000000"
		}
	}
	]
}
